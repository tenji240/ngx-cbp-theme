{
  "name": "ngx-cbp-theme-alt",
  "version": "2.0.6",
  "author": "CBP CF Team / Tenji Tembo",
  "description": "Angular Material Based CBP Compliant Theme",
  "contributors": [
    {
      "name": "Tenji Tembo",
      "email": "tenji240@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tenji240/ngx-cbp-theme.git"
  },
  "keywords": [
    "angular"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/US-CBP/ngx-cbp-theme.git/issues"
  },
  "scripts": {
    "ng": "ng",
    "clean:ngx-cbp-theme": "rimraf dist/ngx-cbp-theme",
    "build:ngx-cbp-theme:watch": "ng build ngx-cbp-theme --watch",
    "build:ngx-cbp-theme": "npm-run-all clean:ngx-cbp-theme build:ngx-cbp-theme:prod  copy-assets:ngx-cbp-theme prepare-package:ngx-cbp-theme",
    "build:ngx-cbp-theme:prod": "ng build ngx-cbp-theme",
    "copy-assets:ngx-cbp-theme": "node scripts/copy-assets.js",
    "prepare-package:ngx-cbp-theme": "node scripts/prepare-package.js",
    "lint": "ng lint",
    "start": "npm-run-all --parallel build:ngx-cbp-theme:watch start:kitchensink",
    "start:kitchensink": "ng serve kitchensink --port 4100 --watch",
    "clean:kitchensink": "rimraf ./dist/kitchensink",
    "build:kitchensink": "ng build kitchensink --prod --base-href ngx-cbp-theme",
    "build:kitchensink:watch": "ng build kitchensink --prod --base-href ngx-cbp-theme --watch",
    "build:kitchensink:prod": "npm-run-all --serial clean:kitchensink build:ngx-cbp-theme build:kitchensink",
    "test:kitchensink": "ng test kitchensink",
    "test:kitchensink:once": "ng test kitchensink --watch=false  --progress=false",
    "test:ngx-cbp-theme": "ng test ngx-cbp-theme",
    "test:ngx-cbp-theme:once": "ng test ngx-cbp-theme --watch=false --progress=false",
    "e2e:kitchensink": "ng e2e kitchensink-e2e --port 4300",
    "ready": "npm-run-all lint build:kitchensink:prod test:once e2e:kitchensink",
    "preversion": "npm run ready",
    "postversion": "npm run prepare-package:ngx-cbp-theme",
    "test:once": "npm-run-all --parallel  test:ngx-cbp-theme:once test:kitchensink:once"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "core-js": "^3.1.3",
    "hammerjs": "^2.0.8",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.5.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.1",
    "@angular-devkit/build-ng-packagr": "~0.800.1",
    "@angular/cli": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.5",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^5.1.0",
    "cpx": "^1.5.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "standard-version": "^7.1.0",
    "tota11y": "^0.1.6",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "^5.20.0",
    "typescript": "~3.4.0",
    "webpack": "^4.40.2"
  }
}
